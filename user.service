[Unit]
Description=User Service

[Service]
User=roboshop
Environment=MONGO=true
# Redis and Mongo configuration
Environment=REDIS_URL=redis://redis.jaswanthdevops.online:6379
Environment=MONGO_URL=mongodb://mongodb.jaswanthdevops.online:27017/users
ExecStart=/bin/node /app/server.js
SyslogIdentifier=user

[Install]
WantedBy=multi-user.target